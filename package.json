{
  "name": "jest-rest-docs",
  "version": "1.0.0",
  "type": "module",
  "description": "A Jest plugin for generating OpenAPI documentation from API tests.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "rollup -c && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts}' 'tests/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' 'tests/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts}' 'tests/**/*.{js,ts}'",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Minseok-2001/jest-rest-docs.git"
  },
  "bugs": {
    "url": "https://github.com/Minseok-2001/jest-rest-docs/issues"
  },
  "homepage": "https://github.com/Minseok-2001/jest-rest-docs#readme",
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rollup": "^2.79.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@types/supertest": "^6.0.2",
    "express": "^4.21.2",
    "fs-extra": "^11.2.0",
    "openapi-types": "^12.1.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2"
  },
  "keywords": [
    "jest",
    "openapi",
    "rest",
    "documentation",
    "api",
    "swagger",
    "jest-swagger",
    "jest-openapi",
    "jest-plugin",
    "jest-plugin-openapi",
    "jest-plugin-swagger"
  ],
  "author": "Minseok-2001<ms.jung.dev@gmail.com>",
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
