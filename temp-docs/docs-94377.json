{
  "paths": {
    "paths": {
      "/api/admin/login": {
        "post": {
          "tags": [
            "Admin"
          ],
          "summary": "관리자 로그인",
          "description": "관리자 계정으로 로그인하여 인증 토큰을 발급받습니다.",
          "parameters": [],
          "responses": {
            "200": {
              "description": "관리자 계정으로 로그인하여 인증 토큰을 발급받습니다.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "token": {
                        "type": "string"
                      }
                    }
                  },
                  "examples": {
                    "example1": {
                      "summary": "관리자 로그인",
                      "value": {
                        "token": "admin-token-123"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/users": {
        "post": {
          "tags": [
            "Users"
          ],
          "summary": "새로운 사용자 생성",
          "description": "신규 사용자를 생성합니다. 이메일은 고유해야 하며, 기본 정보와 주소 정보를 함께 저장할 수 있습니다.",
          "parameters": [],
          "responses": {
            "201": {
              "description": "신규 사용자를 생성합니다. 이메일은 고유해야 하며, 기본 정보와 주소 정보를 함께 저장할 수 있습니다.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "number"
                      },
                      "name": {
                        "type": "string"
                      },
                      "email": {
                        "type": "string"
                      },
                      "age": {
                        "type": "number"
                      },
                      "address": {
                        "type": "object",
                        "properties": {
                          "street": {
                            "type": "string"
                          },
                          "city": {
                            "type": "string"
                          }
                        }
                      },
                      "status": {
                        "type": "string"
                      }
                    }
                  },
                  "examples": {
                    "example1": {
                      "summary": "새로운 사용자 생성",
                      "value": {
                        "id": 1,
                        "name": "홍길동",
                        "email": "hong@example.com",
                        "age": 30,
                        "address": {
                          "street": "테헤란로",
                          "city": "서울"
                        },
                        "status": "active"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/users/search": {
        "get": {
          "tags": [
            "Users"
          ],
          "summary": "사용자 검색",
          "description": "여러 조건으로 사용자를 검색할 수 있습니다.",
          "parameters": [
            {
              "name": "name",
              "in": "query",
              "description": "이름 검색 (부분 일치)",
              "schema": {
                "type": "string"
              },
              "example": "홍"
            },
            {
              "name": "city",
              "in": "query",
              "description": "도시",
              "schema": {
                "type": "string"
              },
              "example": "서울"
            },
            {
              "name": "minAge",
              "in": "query",
              "description": "최소 나이",
              "schema": {
                "type": "integer"
              },
              "example": 20
            },
            {
              "name": "maxAge",
              "in": "query",
              "description": "최대 나이",
              "schema": {
                "type": "integer"
              },
              "example": 40
            }
          ],
          "responses": {
            "200": {
              "description": "여러 조건으로 사용자를 검색할 수 있습니다.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "number"
                        },
                        "name": {
                          "type": "string"
                        },
                        "email": {
                          "type": "string"
                        },
                        "age": {
                          "type": "number"
                        },
                        "address": {
                          "type": "object",
                          "properties": {
                            "street": {
                              "type": "string"
                            },
                            "city": {
                              "type": "string"
                            }
                          }
                        },
                        "status": {
                          "type": "string"
                        }
                      }
                    }
                  },
                  "examples": {
                    "example1": {
                      "summary": "사용자 검색",
                      "value": [
                        {
                          "id": 1,
                          "name": "홍길동",
                          "email": "hong@example.com",
                          "age": 30,
                          "address": {
                            "street": "테헤란로",
                            "city": "서울"
                          },
                          "status": "active"
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/users/{id}": {
        "patch": {
          "tags": [
            "Users"
          ],
          "summary": "사용자 정보 부분 수정",
          "description": "사용자 정보를 부분적으로 수정합니다. 제공된 필드만 업데이트되며, 나머지는 기존 값이 유지됩니다.",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "사용자 ID",
              "required": true,
              "schema": {
                "type": "integer"
              },
              "example": "1"
            }
          ],
          "responses": {
            "200": {
              "description": "사용자 정보를 부분적으로 수정합니다. 제공된 필드만 업데이트되며, 나머지는 기존 값이 유지됩니다.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "number"
                      },
                      "name": {
                        "type": "string"
                      },
                      "email": {
                        "type": "string"
                      },
                      "age": {
                        "type": "number"
                      },
                      "address": {
                        "type": "object",
                        "properties": {
                          "street": {
                            "type": "string"
                          },
                          "city": {
                            "type": "string"
                          }
                        }
                      },
                      "status": {
                        "type": "string"
                      }
                    }
                  },
                  "examples": {
                    "example1": {
                      "summary": "사용자 정보 부분 수정",
                      "value": {
                        "id": 1,
                        "name": "홍길동 (수정됨)",
                        "email": "hong@example.com",
                        "age": 30,
                        "address": {
                          "street": "강남대로",
                          "city": "서울"
                        },
                        "status": "active"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/admin/login": {
      "post": {
        "tags": [
          "Admin"
        ],
        "summary": "관리자 로그인",
        "description": "관리자 계정으로 로그인하여 인증 토큰을 발급받습니다.",
        "parameters": [],
        "responses": {
          "200": {
            "description": "관리자 계정으로 로그인하여 인증 토큰을 발급받습니다.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string"
                    }
                  }
                },
                "examples": {
                  "example1": {
                    "summary": "관리자 로그인",
                    "value": {
                      "token": "admin-token-123"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/users": {
      "post": {
        "tags": [
          "Users"
        ],
        "parameters": [],
        "responses": {
          "400": {
            "description": "400 response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  }
                },
                "examples": {
                  "example1": {
                    "summary": "새로운 사용자 생성",
                    "value": {
                      "error": "Name must be a string"
                    }
                  },
                  "example2": {
                    "summary": "Example 2",
                    "value": {
                      "error": "Invalid email123 format"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Users"
        ],
        "summary": "사용자 목록 조회",
        "description": "페이지네이션과 정렬 기능을 지원하는 사용자 목록 조회 API입니다.",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "페이지 번호",
            "schema": {
              "type": "integer",
              "default": 1
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "페이지당 항목 수",
            "schema": {
              "type": "integer",
              "default": 10,
              "maximum": 100
            }
          },
          {
            "name": "sort",
            "in": "query",
            "description": "정렬 기준 (예: name:asc, name:desc)",
            "schema": {
              "type": "string"
            },
            "example": "name:asc"
          }
        ],
        "responses": {
          "200": {
            "description": "페이지네이션과 정렬 기능을 지원하는 사용자 목록 조회 API입니다.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "users": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "number"
                          },
                          "name": {
                            "type": "string"
                          },
                          "email": {
                            "type": "string"
                          },
                          "age": {
                            "type": "number"
                          },
                          "address": {
                            "type": "object",
                            "properties": {
                              "street": {
                                "type": "string"
                              },
                              "city": {
                                "type": "string"
                              }
                            }
                          },
                          "status": {
                            "type": "string"
                          }
                        }
                      }
                    },
                    "pagination": {
                      "type": "object",
                      "properties": {
                        "total": {
                          "type": "number"
                        },
                        "pages": {
                          "type": "number"
                        },
                        "current": {
                          "type": "number"
                        },
                        "limit": {
                          "type": "number"
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "example1": {
                    "summary": "사용자 목록 조회",
                    "value": {
                      "users": [
                        {
                          "id": 1,
                          "name": "홍길동",
                          "email": "test@example.com",
                          "age": 30,
                          "address": {
                            "street": "테헤란로",
                            "city": "서울"
                          },
                          "status": "active"
                        }
                      ],
                      "pagination": {
                        "total": 1,
                        "pages": 1,
                        "current": 1,
                        "limit": 10
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
